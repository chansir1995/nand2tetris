/**
 * The main class of the Tetris game.
 */
class Main {

    /** Initializes a Pong game and starts running it. */
    function void main() {
        var TetrisGame game;
        do TetrisGame.newInstance();
        let game = TetrisGame.getInstance();
        do game.run();
        //do game.dispose();
        return;
    }
}
